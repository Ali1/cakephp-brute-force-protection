{
    "name": "ali1/cakephp-brute-force-protection",
    "description": "CakePHP Plugin for Brute Force Protection",
    "type": "cakephp-plugin",
    "keywords":[
        "cakephp",
        "plugin",
        "security",
        "bruteforce"
    ],
    "homepage": "https://github.com/ali1/cakephp-brute-force-protection",
    "license": "MIT",
    "authors": [
        {
            "name": "Ali Bakir",
            "homepage": "https://github.com/ali1/"
        }
    ],
    "require": {
        "php": ">=5.4.16",
        "cakephp/cakephp": "~3.0",
        "mockery/mockery": "0.9.*"
    },
    "require-dev": {
        "phpunit/phpunit": "*"
    },
    "autoload": {
        "psr-4": {
            "BruteForceProtection\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BruteForceProtection\\Test\\": "tests",
            "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
        }
    }
}
