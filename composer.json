{
    "name": "ali1/cakephp-brute-force-protection",
    "description": "CakePHP Plugin for Brute Force Protection",
    "type": "cakephp-plugin",
    "keywords":[
        "cakephp",
        "plugin",
        "security",
        "bruteforce"
    ],
    "homepage": "https://github.com/ali1/cakephp-brute-force-protection",
    "license": "MIT",
    "authors": [
        {
            "name": "Ali Bakir",
            "homepage": "https://github.com/ali1/"
        }
    ],
    "require": {
        "php": ">=7.2",
        "cakephp/cakephp": "^4.0",
        "mockery/mockery": "0.9.*"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "BruteForceProtection\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BruteForceProtection\\": "src",
            "TestApp\\": "tests/TestApp/src/"
        }
    }
}
